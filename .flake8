[flake8]
max-line-length = 100
extend-ignore = 
    E203
    W503
    W291
    W293
    E501
    F401
    F841
    E722
    W605
    E731
    F403
    F405
    E402
    C901
exclude = 
    .git
    __pycache__
    .venv
    venv
    build
    dist
    .eggs
    *.egg-info
    .tox
    .mypy_cache
    docs/_build
    htmlcov
per-file-ignores =
    __init__.py:F401
    tests/*:E501
    src/callbacks/*:C901
